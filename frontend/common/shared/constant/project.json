{
  "name": "constant",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "common/shared/constant/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/common/shared/constant",
        "main": "common/shared/constant/src/index.ts",
        "tsConfig": "common/shared/constant/tsconfig.lib.json",
        "assets": ["common/shared/constant/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "common/shared/constant/jest.config.ts"
      }
    }
  }
}
